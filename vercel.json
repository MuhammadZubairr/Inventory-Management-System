{
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/FrontEnd/assets/$1" },
    { "source": "/components/(.*)", "destination": "/FrontEnd/components/$1" },
    { "source": "/pages/(.*)", "destination": "/FrontEnd/pages/$1" },
    { "source": "/", "destination": "/FrontEnd/pages/login.html" },
    { "source": "/admin.html", "destination": "/FrontEnd/pages/admin.html" },
    { "source": "/user-dashboard.html", "destination": "/FrontEnd/pages/user-dashboard.html" },
    { "source": "/user-login.html", "destination": "/FrontEnd/pages/user-login.html" },
    { "source": "/user-stock-in.html", "destination": "/FrontEnd/pages/user-stock-in.html" },
    { "source": "/user-stock-out.html", "destination": "/FrontEnd/pages/user-stock-out.html" },
    { "source": "/user-products.html", "destination": "/FrontEnd/pages/user-products.html" },
    { "source": "/(.*).html", "destination": "/FrontEnd/pages/$1.html" },
    { "source": "/(.*)", "destination": "/FrontEnd/pages/$1" }
  ]
}