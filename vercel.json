{
  "version": 2,
  "buildCommand": "cd FrontEnd && echo 'Build complete'",
  "outputDirectory": "FrontEnd",
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/components/(.*)",
      "destination": "/components/$1"
    },
    {
      "source": "/pages/(.*)",
      "destination": "/pages/$1"
    },
    {
      "source": "/",
      "destination": "/pages/login.html"
    },
    {
      "source": "/(.*)",
      "destination": "/pages/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}